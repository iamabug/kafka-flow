<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>config</servlet-name>
        <jsp-file>/config.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>config</servlet-name>
        <url-pattern>/config</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>produce</servlet-name>
        <jsp-file>/produce.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>produce</servlet-name>
        <url-pattern>/produce</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>consume</servlet-name>
        <jsp-file>/consume.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>consume</servlet-name>
        <url-pattern>/consume</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>new_config</servlet-name>
        <jsp-file>/new_config.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>new_config</servlet-name>
        <url-pattern>/new_config</url-pattern>
    </servlet-mapping>
        </web-app>